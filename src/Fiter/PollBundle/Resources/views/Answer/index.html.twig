{% extends '::base.html.twig' %}
 
{% block title %}Answer list{% endblock %}

{% block main %} 
    <h1>Answer list</h1>
    {% for entity in entities %}
        Id: <td><a href="{{ path('answer_show', { 'id': entity.id }) }}">{{ entity.id }}</a><br/>
        <!-- Value: <td>{#{ entity.value }#}<br/> -->
        {{ entity.answerGroup.author }} - {{ entity.answerGroup.clientIp }}<br/>
        <a class="boton" href="{{ path('answer_show', { 'id': entity.id }) }}">show</a>
        <a class="boton" href="{{ path('answer_edit', { 'id': entity.id }) }}">edit</a>
        <br/><br/>
    {% endfor %}
    <!-- <a class="boton" href="{#{ path('answer_new') }#}">Create a new entry</a> -->
{% endblock %}


