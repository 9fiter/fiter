<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Fiter\MinecraftBundle\Entity\Jobs" table="jobs_jobs">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="20" nullable="false"/>
    <field name="experience" type="integer" column="experience" nullable="true"/>
    <field name="level" type="integer" column="level" nullable="true"/>
    <field name="job" type="string" column="job" length="20" nullable="false"/>
  </entity>
</doctrine-mapping>
