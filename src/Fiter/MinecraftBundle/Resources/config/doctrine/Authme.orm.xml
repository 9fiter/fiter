<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Fiter\MinecraftBundle\Entity\Authme" table="authme">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="255" nullable="false"/>
    <field name="password" type="string" column="password" length="255" nullable="false"/>
    <field name="ip" type="string" column="ip" length="40" nullable="false"/>
    <field name="lastlogin" type="bigint" column="lastlogin" nullable="true"/>
    <field name="x" type="smallint" column="x" nullable="true"/>
    <field name="y" type="smallint" column="y" nullable="true"/>
    <field name="z" type="smallint" column="z" nullable="true"/>
    <field name="world" type="string" column="world" length="255" nullable="true"/>
    <field name="email" type="string" column="email" length="255" nullable="false"/>
    <field name="countrycode" type="string" column="countryCode" length="255" nullable="true"/>
    <field name="countrycode3" type="string" column="countryCode3" length="255" nullable="true"/>
    <field name="countryname" type="string" column="countryName" length="255" nullable="true"/>
    <field name="region" type="string" column="region" length="255" nullable="true"/>
    <field name="city" type="string" column="city" length="255" nullable="true"/>
    <field name="postalcode" type="string" column="postalCode" length="255" nullable="true"/>
    <field name="latitude" type="string" column="latitude" length="255" nullable="true"/>
    <field name="longitude" type="string" column="longitude" length="255" nullable="true"/>
    <field name="areacode" type="string" column="areaCode" length="255" nullable="true"/>
    <field name="metrocode" type="string" column="metroCode" length="255" nullable="true"/>
    <field name="continentcode" type="string" column="continentCode" length="255" nullable="true"/>
    <field name="created" type="datetime" column="created" nullable="false"/>
    <field name="skin" type="integer" column="skin" nullable="false"/>
    <field name="premium" type="boolean" column="premium" nullable="false"/>
  </entity>
</doctrine-mapping>
